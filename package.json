  },
  "homepage": "https://github.com/ETHproductions/infimath#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.4",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^3.0.0"
  },
  "private": true
